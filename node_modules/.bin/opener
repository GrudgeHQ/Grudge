#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/rozwe/OneDrive/Documents/Grudge App/Grudge2/node_modules/opener/bin/node_modules:/mnt/c/Users/rozwe/OneDrive/Documents/Grudge App/Grudge2/node_modules/opener/node_modules:/mnt/c/Users/rozwe/OneDrive/Documents/Grudge App/Grudge2/node_modules:/mnt/c/Users/rozwe/OneDrive/Documents/Grudge App/node_modules:/mnt/c/Users/rozwe/OneDrive/Documents/node_modules:/mnt/c/Users/rozwe/OneDrive/node_modules:/mnt/c/Users/rozwe/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/rozwe/OneDrive/Documents/Grudge App/Grudge2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/rozwe/OneDrive/Documents/Grudge App/Grudge2/node_modules/opener/bin/node_modules:/mnt/c/Users/rozwe/OneDrive/Documents/Grudge App/Grudge2/node_modules/opener/node_modules:/mnt/c/Users/rozwe/OneDrive/Documents/Grudge App/Grudge2/node_modules:/mnt/c/Users/rozwe/OneDrive/Documents/Grudge App/node_modules:/mnt/c/Users/rozwe/OneDrive/Documents/node_modules:/mnt/c/Users/rozwe/OneDrive/node_modules:/mnt/c/Users/rozwe/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/rozwe/OneDrive/Documents/Grudge App/Grudge2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../opener/bin/opener-bin.js" "$@"
else
  exec node  "$basedir/../opener/bin/opener-bin.js" "$@"
fi
